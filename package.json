{
  "name": "alclient",
  "type": "module",
  "version": "1.0.0",
  "description": "A Node client for Adventureland (MMORPG)",
  "files": [
    "dist/*",
    "package.json",
    "README.md"
  ],
  "main": "dist/Index.js",
  "engines": {
    "node": ">=21"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -r dist && rm ./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "format": "prettier --write .",
    "lint": "eslint",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --maxWorkers=2 --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthiverse/alclient.git"
  },
  "keywords": [
    "alclient",
    "adventureland"
  ],
  "author": "Kent Rasmussen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/earthiverse/alclient/issues"
  },
  "homepage": "https://github.com/earthiverse/alclient#readme",
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.25.1",
    "eslint-plugin-jest": "^29.2.1",
    "jest": "^30.2.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.0.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  },
  "dependencies": {
    "alclient": "file:alclient-1.0.0.tgz",
    "alpathfinder": "^0.2.6",
    "socket.io-client": "^4.7.5",
    "typed-adventureland": "^0.0.52"
  }
}
